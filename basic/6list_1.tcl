#! /usr/bin/tclsh 

set inputs [list c d a b ]

  puts "[lindex $inputs 0]"
  puts "[lindex $inputs 1]"
  puts "[lindex $inputs 2]"
  puts "[lindex $inputs 3]"

  puts "[lindex $inputs ]"

  puts "SIZE OF THE LIST ::[llength $inputs]"
  
  puts "SORT ::[lsort $inputs]"
  puts "REVERSE ::[lreverse $inputs]"

  

set outputs {x y g z }

  puts "[lindex $outputs ]"
